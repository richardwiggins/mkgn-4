	<script>document.getElementsByTagName("html")[0].className += " js";</script>
	<script>
		if('CSS' in window && CSS.supports('color', 'var(--color-var)')) {
			document.write('<link rel="stylesheet" href="{{ siteUrl }}assets/css/style.css">');
		} else {
			document.write('<link rel="stylesheet" href="{{ siteUrl }}assets/css/style-fallback.css">');
		}
	</script>
	<noscript>
		<link rel="stylesheet" href="{{ siteUrl }}assets/css/style-fallback.css">
	</noscript>
	<title>MKGN</title>
</head>
<body>

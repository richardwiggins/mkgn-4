{
  "require": {
    "aelvan/imager": "v2.4.0",
    "craftcms/cms": "3.7.8",
    "craftcms/redactor": "2.8.8",
    "solspace/craft-freeform": "3.11.10",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4.9"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
